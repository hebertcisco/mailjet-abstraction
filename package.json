{
  "name": "mailjet-abstraction",
  "version": "0.0.3",
  "description": "Build Together. Send Smarter.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "test": "jest --config jest.config.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "node-mailjet": "^3.0.5"
  },
  "devDependencies": {
    "@types/chance": "^1.1.3",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^18.0.0",
    "@types/node-mailjet": "3.3.8",
    "@typescript-eslint/eslint-plugin": "5.48.0",
    "@typescript-eslint/parser": "5.47.1",
    "chance": "^1.1.8",
    "eslint": "^8.29.0",
    "eslint-plugin-jest": "27.2.0",
    "jest": "^26.6.0",
    "prettier": "^2.7.1",
    "ts-jest": "^26.4.1",
    "typescript": "4.9.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hebertcisco/ts-npm-package-boilerplate.git"
  },
  "keywords": [
    "mailjet",
    "typescript",
    "mail",
    "email"
  ],
  "author": "Hebert Cisco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hebertcisco/mailjet-abstraction/issues"
  },
  "homepage": "https://github.com/hebertcisco/mailjet-abstraction#readme",
  "files": [
    "lib/**/*"
  ]
}
